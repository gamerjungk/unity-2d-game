    "configurations": [
        
        {
            "name": "C#: <project-name> Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/<relative-path-to-project-folder><project-name>.csproj"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },

        {
            "type": "node-terminal",
            "name": "JavaScript 디버그 터미널",
            "request": "launch",
            "cwd": "${workspaceFolder}"
        }
    ]
